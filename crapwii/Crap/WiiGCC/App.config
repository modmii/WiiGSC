    <add key="IpAddressOfWii" value="192.168.2.5" />
    <add key="GameBlockageType" value="1"/>
    <add key="SelectedNandLoader" value="experimental-base.wxd"/>
    <add key="BannerLanguagePreference" value="1,2,3,4,0,5"/>
    <add key="Disclaimer" value="on"/>
    <add key="UseProxy" value="off"/>
    <add key="ProxyServer" value=""/>
    <add key="ProxyUser" value=""/>
    <add key="ProxyPass" value=""/> 
  </appSettings>
</configuration>
